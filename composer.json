{
    "name": "technosun/ckfinder-laravel-package",
    "description": "CKFinder 3 package for Laravel",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.6.0",
        "laravel/framework": "^5.5|^6.20.42|^7.0|^8.0|^9.0",
        "pimple/pimple": "~3.0",
        "technosun/flysystem": "dev-master",
        "technosun/flysystem-cached-adapter": "dev-master",
        "league/flysystem-aws-s3-v3": "^3.0",
        "spatie/flysystem-dropbox": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "CKSource\\CKFinderBridge\\": "src/",
            "CKSource\\CKFinder\\": "_connector/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CKSource\\CKFinderBridge\\CKFinderServiceProvider"
            ]
        }
    }
}
